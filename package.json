{
  "name": "@wangankeji/vue-g6editor",
  "description": "Editor for G6 graph.",
  "version": "0.1.0",
  "main": "dist/g6editor.common.js",
  "scripts": {
    "serve": "npx vue-cli-service serve",
    "build": "npx vue-cli-service build --target lib --dest dist --formats commonjs --name g6editor src/components/index.js",
    "release": "yarn run lint & yarn run build & node version.js",
    "lint": "npx vue-cli-service lint",
    "test": "npx vue-cli-service lint & npx vue-cli-service build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitee.com/wangankeji/vue-g6editor.git"
  },
  "bugs": {
    "url": "https://gitee.com/wangankeji/vue-g6editor/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "homepage": "https://wangankeji.gitee.io/vue-g6editor/",
  "keywords": [
    "vue",
    "g6",
    "editor"
  ],
  "author": "wangankeji",
  "license": "MIT",
  "files": [
    "dist",
    "src/components",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@antv/g6": "^4.8.22",
    "uuid": "^9.0.1"
  },
  "peerDependencies": {},
  "optionalDependencies": {},
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-service": "^4.2.0",
    "babel-eslint": "^10.0.3",
    "core-js": "^3.6.4",
    "element-ui": "^2.15.14",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-console": "warn",
      "no-debugger": "warn"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
